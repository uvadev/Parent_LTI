<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title -Parent Analytics Beta</title>
    @Styles.Render("~/Content/Site.css")
</head>

<body translate="no">
    @RenderBody()
    <footer>
        <p>@DateTime.Now.Year - @ViewBag.Title</p>
    </footer>
    @RenderSection("scripts", required: false)
</body>
</html>